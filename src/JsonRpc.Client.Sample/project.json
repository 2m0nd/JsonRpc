{
	"title": "JsonRpc DNX Client Sample",
	"version": "1.0.0-rc2-001",
	"description": "A Client sample for Json Rpc v2 requests for .Net Core.",
	"packOptions": {
		"summary": "A Json RPC v2 client sample for .Net Core",
		"tags": [ "JsonRpc Rpc AspNet Json-Rpc .Net Core" ],
		"projectUrl": "https://github.com/Gekctek/JsonRpc.Router",
		"repository": { "type": "git" },
		"licenseUrl": "https://raw.githubusercontent.com/Gekctek/JsonRpc.Router/master/LICENSE",
		"releaseNotes": "Upgrading to rc2"
	},
	"authors": [ "Gekctek" ],

	"dependencies": {
		"JsonRpc.Client": {
			"target": "project",
			"version": "1.0.0-*"
		}
	},

	"frameworks": {
		"dnx451": { },
		"netcoreapp1.0": {
			"dependencies": {
				"Microsoft.NETCore.App": {
					"version": "1.0.0-rc2-3002702",
					"type": "platform"
				},
				"System.Collections": "4.0.11-rc2-24027",
				"System.Runtime": "4.1.0-rc2-24027",
				"System.Runtime.Serialization.Primitives": "4.1.1-rc2-24027",
				"System.Threading": "4.0.11-rc2-24027",
				"System.IO.Compression": "4.1.0-rc2-24027",
				"System.Console": "4.0.0-rc2-24027"
			},
			"imports": [
				"dotnet52",
				"dnxcore50",
				"portable-net45+win8"
			]
		}
	},

	"buildOptions": {
		"emitEntryPoint": true,
		"preserveCompilationContext": true
	},

	"runtimeOptions": {
		"gcServer": true
	}
}
